<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/main_image_boss_previous_one"
        android:layout_width="75dp"
        android:layout_height="75dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.047"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.024"
        app:srcCompat="@drawable/vell_big" />

    <ImageView
        android:id="@+id/main_image_boss_previous_two"
        android:layout_width="75dp"
        android:layout_height="75dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.27"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.024"
        app:srcCompat="@drawable/vell_big" />

    <TextView
        android:id="@+id/main_text_boss_title_previous"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@id/main_image_boss_previous_two"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.056" />

    <TextView
        android:id="@+id/main_text_boss_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.238" />

    <ImageView
        android:id="@+id/main_image_boss_one"
        android:layout_width="175dp"
        android:layout_height="175dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/vell_big" />

    <ImageView
        android:id="@+id/main_image_boss_two"
        android:layout_width="175dp"
        android:layout_height="175dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.814"
        app:srcCompat="@drawable/vell_big" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:weightSum="10"
        android:gravity="center"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.95">
    
    <ImageView
        android:id="@+id/main_image_boss_setting_kzarka"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:padding="5dp"
        android:layout_margin="1dp"
        android:background="@drawable/button_green"
        app:srcCompat="@drawable/kzarka_big" />

    <ImageView
        android:id="@+id/main_image_boss_setting_karanda"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:padding="5dp"
        android:layout_margin="1dp"
        android:background="@drawable/button_green"
        app:srcCompat="@drawable/karanda_big" />

    <ImageView
        android:id="@+id/main_image_boss_setting_kutum"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:padding="5dp"
        android:layout_margin="1dp"
        android:background="@drawable/button_green"
        app:srcCompat="@drawable/kutum_big" />

    <ImageView
        android:id="@+id/main_image_boss_setting_nouver"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:padding="5dp"
        android:layout_margin="1dp"
        android:background="@drawable/button_green"
        app:srcCompat="@drawable/nouver_big" />

    <ImageView
        android:id="@+id/main_image_boss_setting_offin"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:padding="5dp"
        android:layout_margin="1dp"
        android:background="@drawable/button_green"
        app:srcCompat="@drawable/offin_big" />

    <ImageView
        android:id="@+id/main_image_boss_setting_garmoth"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:padding="5dp"
        android:layout_margin="1dp"
        android:background="@drawable/button_green"
        app:srcCompat="@drawable/garmoth_big" />

    <ImageView
        android:id="@+id/main_image_boss_setting_quint"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:padding="5dp"
        android:layout_margin="1dp"
        android:background="@drawable/button_green"
        app:srcCompat="@drawable/quint_big" />

    <ImageView
        android:id="@+id/main_image_boss_setting_muraka"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:padding="5dp"
        android:layout_margin="1dp"
        android:background="@drawable/button_green"
        app:srcCompat="@drawable/muraka_big" />

    <ImageView
        android:id="@+id/main_image_boss_setting_vell"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:padding="5dp"
        android:layout_margin="1dp"
        android:background="@drawable/button_green"
        app:srcCompat="@drawable/vell_big" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>